/**app.wxss**/
page {
  background: #f9f9f9;
}

.bg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 144rpx;
  z-index: -1;
}

.unLoginCard{
  background: #fff;
  margin: 50rpx 30rpx;
  border-radius: 13rpx;
  box-shadow: 1rpx 1rpx 50rpx 5rpx #eaeaea;
  position: relative;
  height: 200rpx;
  display: flex;
}
.unLoginHead{
  border-radius: 50%;
  flex-shrink: 0;
  height: 100rpx;
  width: 100rpx;
  background: #F5E069;
  position: absolute;
  top: 50rpx;
  left: 50rpx;
}
.unLoginInfo{
  position: absolute;
  flex: 1;
  top: 50rpx;
  left: 200rpx;
}
.unLoginInfoText{
  font-size: 35rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
}
.unLoginInfoTips{
  font-size: 25rpx;
}
.unLoginBtn{
  border-radius: 13rpx;
  width: 250rpx;
  height: 80rpx;
  font-size: 35rpx;
  line-height: 80rpx;
  font-weight: bold;
  margin: 34rpx auto 0;
  box-shadow: 1rpx 1rpx 50rpx 5rpx #eaeaea;
  background: #F7E687;
  align-items: center;
  text-align: center;
}